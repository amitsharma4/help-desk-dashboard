<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>reg</servlet-name>
        <servlet-class>reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>queries</servlet-name>
        <servlet-class>queries</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>index1</servlet-name>
        <servlet-class>index1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedback</servlet-name>
        <servlet-class>feedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>expert</servlet-name>
        <servlet-class>expert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>type</servlet-name>
        <servlet-class>type</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>regexp</servlet-name>
        <servlet-class>regexp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reg</servlet-name>
        <url-pattern>/reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>queries</servlet-name>
        <url-pattern>/queries</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index1</servlet-name>
        <url-pattern>/index1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedback</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>expert</servlet-name>
        <url-pattern>/expert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>type</servlet-name>
        <url-pattern>/type</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>regexp</servlet-name>
        <url-pattern>/regexp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
